<div class='container'>
    {{#each course}}
        <div class='card' style='width: 18rem;'>
            <a href='/'><img
                    src={{this.image}}
                    class='card-img-top'
                    alt={{this.slug}}
                /></a>
            <div class='card-body'>
                <a href='/' class='card-title'>{{this.name}}</a>
                <p class='card-text'>{{this.description}}</p>
            </div>
        </div>
    {{/each}}
</div>